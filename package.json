{
  "private": true,
  "engines": {
    "node": "5.11.0",
    "npm": "3.8.6"
  },
  "scripts": {
    "start": "nodemon ./bin/www",
    "watch": "webpack -w",
    "build:dll": "webpack --config webpack.dll.js",
    "postinstall": "npm run production",
    "production": "NODE_ENV='production' && webpack -p",
    "test": "NODE_ENV=test mocha --require babel-core/register app/services/**/*.test.js",
    "test-report": "NODE_ENV=test mocha --recursive -R xunit tests/browser.js tests/**/*.test.js > test-reports.xml",
    "test-watch": "nodemon --watch app/services/**/*.js -q -x 'npm test'",
    "flow": "flow",
    "flow-coverage-report": "flow-coverage-report -i 'resources/assets/js/lib/**/*.js' -t html -t text; open flow-coverage/index.html"
  },
