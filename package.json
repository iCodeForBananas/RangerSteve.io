  "scripts": {
    "start": "nodejs-dashboard -- nodemon ./bin/www",
    "watch": "webpack-dashboard -p 9839 -- webpack -w --config webpack.dev.js",
    "build:dll": "webpack --config webpack.dll.js",
    "postinstall": "npm run build:dll",
    "production": "NODE_ENV='production' && webpack --config ./webpack.production.js && node bin/www",
    "test": "NODE_ENV=test mocha tests/browser.js tests/ui/**/*.test.js tests/lib/**/*.test.js",
    "test-report": "NODE_ENV=test mocha --recursive -R xunit tests/browser.js tests/**/*.test.js > test-reports.xml",
    "test-watch": "nodemon --watch tests/ui/**/*.test.js --watch tests/lib/**/*.test.js --watch resources/assets/js/ui/**/*.js --watch resources/assets/js/lib/Percent.js -q -x 'npm test'"
  },
