{
  "private": true,
  "engines": {
    "node": "6.9.4",
    "npm": "3.10.10"
  },
  "scripts": {
    "start": "nodemon ./bin/www",
    "postinstall": "npm run build:prod",
    "test": "NODE_ENV=test mocha --require babel-core/register lib/**/*.test.js && standard",
    "test-report": "NODE_ENV=test mocha --recursive -R xunit tests/browser.js tests/**/*.test.js > test-reports.xml",
    "test-watch": "nodemon --watch app/services/**/*.js -q -x 'npm test'",
    "watch": "webpack --env dev -w",
    "build:dev": "webpack --env dev --progress --profile --colors",
    "build:prod": "webpack --env prod --progress --profile --colors"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it"
    ]
  },
