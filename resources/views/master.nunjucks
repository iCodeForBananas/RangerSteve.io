    {% block head %}{% endblock %}
</head>
<body class="main">
    <nav class="navbar navbar-default navbar-fixed-top" style="background: white;">
        <div class="container">
            <div class="row">
                <div class="col-sm-3">
                    <div class="navbar-header">
                        <a
                            class="navbar-brand"
                            href="/"
                        >
                            Ranger Steve
                        </a>
                    </div>
                </div>
                <div class="col-sm-6 align-middle">
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li><a href="/game">Play Now</a></li>
                            <li><a href="/gallery">Gallery</a></li>
                            <li><a href="/blog">What's New</a></li>
                            <li><a href="/credits">Credits</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-sm-3 text-center">
                    <a
                        class="social-link social-link-twitter"
                        href="https://twitter.com/michaeljcalkins"
                        target="_blank"
                    >
                        <i class="fa fa-twitter"></i>
                    </a>
                    <a
                        class="social-link social-link-facebook"
                        href="https://www.facebook.com/rangerstevegame"
                        target="_blank"
                    >
                        <i class="fa fa-facebook"></i>
                    </a>
                </div>
            </div>
        </div>
    </nav>

    {% block content %}{% endblock %}
</body>
</html>
