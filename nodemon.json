{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
  },
  "watch": [
    "bin/",
    "app/**/*.js",
    "app.js",
    "lib/**/*.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json njk nunjucks md"
}
