{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
  },
  "watch": [
    "bin/",
    "public/app.js",
    "resources/assets",
    "app/",
    "app.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json nunjucks md"
}
